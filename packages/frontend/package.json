{
	"name": "@firma-sign/frontend",
	"version": "0.0.1",
	"description": "Frontend package for firma-sign",
	"main": "index.js",
	"scripts": {
		"build": "vite build",
		"dev": "vite",
		"preview": "vite preview",
		"test": "vitest",
		"format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,html}\" --ignore-unknown || true",
		"format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,html}\" --ignore-unknown || true",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lintfix": "eslint . --ext ts,tsx --fix"
	},
	"dependencies": {
		"react": "catalog:frontend",
		"react-dom": "catalog:frontend",
		"@firma-sign/documents": "workspace:*"
	},
	"devDependencies": {
		"@types/react": "catalog:frontend",
		"@types/react-dom": "catalog:frontend",
		"@vitejs/plugin-react": "catalog:frontend",
		"typescript": "catalog:",
		"vite": "catalog:",
		"vitest": "catalog:",
		"eslint": "catalog:",
		"tailwindcss": "catalog:frontend",
		"@tailwindcss/postcss": "catalog:frontend",
		"autoprefixer": "catalog:frontend",
		"postcss": "catalog:frontend"
	},
	"keywords": [],
	"author": "cto@firmachain.com",
	"license": "MIT"
}
